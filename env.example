# Database (Railway PostgreSQL)
DATABASE_URL=postgresql://user:password@host:port/database

# Encryption (32-byte hex key for AES-256 - generate with: openssl rand -hex 32)
ENCRYPTION_KEY=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef

# Claude API
ANTHROPIC_API_KEY=sk-ant-your-key-here

# Solana RPC (Helius recommended for reliability)
HELIUS_API_KEY=your-helius-api-key
NEXT_PUBLIC_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your-helius-api-key

# Birdeye API (for OHLCV/technical indicators - get key at birdeye.so)
BIRDEYE_API_KEY=your-birdeye-api-key

# Jupiter API (no key required - free)
# Endpoints: https://quote-api.jup.ag/v6/

# Pump.fun API (no key required - free)
# Endpoints: https://frontend-api.pump.fun/

# DexScreener API (no key required - free)
# Endpoints: https://api.dexscreener.com/latest/

# Withdrawal Settings
MIN_WITHDRAWAL_SOL=0.01
WITHDRAWAL_FEE_SOL=0.001
MAX_DAILY_WITHDRAWALS=10

# Logging
LOG_LEVEL=info
